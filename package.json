{
  "name": "greenbin-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:greenbin-front": "node dist/greenbin-front/server/server.mjs",
    "prepare": "husky install",
